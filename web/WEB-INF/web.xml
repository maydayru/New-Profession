<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <description></description>
        <display-name>SubjectList</display-name>
        <servlet-name>SubjectList</servlet-name>
        <servlet-class>MainPackage.SubjectListpkg.SubjectList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubjectList</servlet-name>
        <url-pattern>/ShowMeSubjectList</url-pattern>
    </servlet-mapping>
    <servlet>
        <description></description>
        <display-name>DirectionList</display-name>
        <servlet-name>DirectionList</servlet-name>
        <servlet-class>MainPackage.DirectionListpkg.DirectionList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DirectionList</servlet-name>
        <url-pattern>/ShowMeDirectionList</url-pattern>
    </servlet-mapping>


    <servlet>
        <description></description>
        <display-name>LecturerList</display-name>
        <servlet-name>LecturerList</servlet-name>
        <servlet-class>MainPackage.LecturerListpkg.LecturerList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LecturerList</servlet-name>
        <url-pattern>/ShowMeLecturerList</url-pattern>
    </servlet-mapping>



    <servlet>
        <display-name>AddCourse</display-name>
        <servlet-name>AddCourse</servlet-name>
        <servlet-class>Adding.Course.AddCourse</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddCourse</servlet-name>
        <url-pattern>/AddNewCourse</url-pattern>
    </servlet-mapping>

</web-app>
